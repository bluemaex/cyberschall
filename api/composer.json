        "ramsey/uuid": "^3.7",
        "silex/silex": "~2.2",
        "symfony/finder": "^3.3",
        "symfony/security": "^3.3",
        "symfony/validator": "^3.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.5",
        "phpmd/phpmd": "^2.6",
